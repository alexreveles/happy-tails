<h3>Hello! Here are your daily doggy activities:</h3>
<br>
<h4>AM Shift</h4>

<div class="row s8">
    <table class="striped">
        <thead>
            <tr>
                <th>Doggy Name</th>
                <th>AM Potty Break</th>
                <th>AM Walk</th>
                <th>Doggy Demeanor</th>
                <th>Kennel</th>
            </tr>
        </thead>
        <tbody>
            {{#each canine}}
            {{>dog-info-am}}
            {{/each}}

        </tbody>
        </tr>
        </tbody>
    </table>

    <br>
    <h4 class="center underline">PM Shift</h4>

    <table class="striped">
        <thead>
            <tr>
                <th>Doggy Name</th>
                <th>PM Potty Break</th>
                <th>PM Walk</th>
                <th>Doggy Demeanor</th>
                <th>Kennel</th>
            </tr>
        </thead>
        <tbody>
            {{#each canine}}
            {{>dog-info-pm}}
            {{/each}}

        </tbody>
        </tr>
        </tbody>
    </table>
</div>